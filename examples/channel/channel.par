[OCCA]
backend = CUDA
deviceNumber = LOCAL-RANK

[GENERAL]
polynomialOrder = 7
#startFrom = r.fld+time=0
stopAt = endTime
endTime = 1
dt = 1e-03
timeStepper = tombo3

writeControl = runTime
writeInterval = 50

[PRESSURE]
residualTol = 1e-10

[VELOCITY]
boundaryTypeMap = inlet, wall, outlet
residualTol = 1e-12
density = 1.0
viscosity = -100. 
